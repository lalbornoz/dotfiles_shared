include "%L"			# locale specific Compose file (i.e., "/usr/share/X11/locale/<localename>/Compose")

<Multi_key> <3> <apostrophe>				: "ʔ"	U0294 # LATIN LETTER GLOTTAL STOP
<Multi_key> <3> <3>					: "ʕ"	U0295 # LATIN LETTER PHARYNGEAL VOICED FRICATIVE
<Multi_key> <underscore> <H>				: "Ḫ"	U1E2A # LATIN CAPITAL LETTER H WITH BREVE BELOW
<Multi_key> <underscore> <h>				: "ḫ"	U1E2B # LATIN SMALL LETTER H WITH BREVE BELOW
<Multi_key> <underscore> <T>				: "Ṯ"   U1E6E # LATIN CAPITAL LETTER T WITH LINE BELOW
<Multi_key> <underscore> <t>				: "ṯ"   U1E6F # LATIN SMALL LETTER T WITH LINE BELOW
<Multi_key> <underscore> <D>				: "Ḏ"   U1E6E # LATIN CAPITAL LETTER D WITH LINE BELOW
<Multi_key> <underscore> <d>				: "ḏ"   U1E6F # LATIN SMALL LETTER D WITH LINE BELOW

